  "main": "index.js",
  "scripts": {
    "dev:up": "docker-compose -f docker/docker-compose.dev.yaml up --abort-on-container-exit --remove-orphans",
    "dev:halt": "docker-compose -f docker/docker-compose.dev.yaml down",
    "dev:destroy": "docker-compose -f docker/docker-compose.dev.yaml rm -f",
    "dev:log": "docker-compose -f docker/docker-compose.dev.yaml logs -f",
    "dev:rmq": "docker-compose -f docker/docker-compose.dev.yaml restart fl-rabbitmq"
